  "scripts": {
    "tailwind:prodbuild": "NODE_ENV=production npm run tailwind:build",
    "tailwind:build": "npx tailwindcss -i ./src/styles/tailwind.css -o ./src/styles/styles.css --watch",
    "apollo:codegen": "apollo client:codegen --target=typescript --outputFlat",
    "start": "npm run tailwind:build & react-scripts start",
    "build": "CI=false react-scripts build",
    "prebuild": "npm run tailwind:prodbuild",
    "test": "react-scripts test --verbose",
    "test:coverage": "npm test -- --coverage --watchAll=false",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@testing-library/cypress": "^8.0.3",
    "@types/google.maps": "^3.49.2",
    "autoprefixer": "^10.4.7",
    "cypress": "^10.4.0",
    "mock-apollo-client": "^1.2.0",
    "postcss": "^8.4.14",
    "tailwindcss": "^3.1.6"
  },
  "jest": {
    "collectCoverageFrom": [
      "./src/components/**/*.tsx",
      "./src/pages/**/*.tsx",
      "./src/routers/**/*.tsx"
    ]
  }
